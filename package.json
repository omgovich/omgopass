{
  "name": "omgopass",
  "version": "2.1.0",
  "description": "A tiny memorable password generator for Node.js and browser",
  "keywords": [
    "random",
    "password",
    "generator"
  ],
  "scripts": {
    "test": "npm run bundle-cjs && jest",
    "size-limit": "size-limit",
    "bundle-cjs": "rollup index.js --o cjs/index.js --f cjs",
    "prepublishOnly": "npm run bundle-cjs"
  },
  "module": "index.js",
  "main": "cjs/index.js",
  "repository": "omgovich/omgopass",
  "author": "Vlad Shilov <omgovich@ya.ru>",
  "license": "ISC",
  "size-limit": [
    {
      "path": "index.js",
      "limit": "350 B"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^2.1.6",
    "jest": "^24.9.0",
    "rollup": "^1.21.4"
  }
}
